{
  "name": "ai-toolkit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,js}' --max-warnings=0",
    "format": "prettier --write 'src/**/*.{ts,js,md,json}'",
    "pack": "npm pack",
    "release": "echo 'Run release pipeline (CI) to publish'",
    "serve": "node server.js"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "dotenv": "^16.0.0",
    "openai": "^4.0.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0",
    "eslint-config-prettier": "^8.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint-plugin-import": "^2.0.0"
  }
}
